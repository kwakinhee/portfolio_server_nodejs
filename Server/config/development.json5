// dev 환경
{
  // 기본 로깅 설정
  log: {
    // 콘솔 출력
    console: {
      level: "info",
      inspectDepth: 7,
    },

    // 파일 출력
    file: {
      level: "info",
      // 로그파일 Rotation 설정.
      datePattern: "YYYY-MM-DD.HH",
      datePatternDaily: "YYYY-MM-DD",
